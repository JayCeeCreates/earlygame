{
    "schemaVersion": 1,
    "id": "earlygame",
    "version": "${version}",

    "name": "EarlyGame",
    "description": "A Fabric mod that focuses on changing early game progression that makes the gameplay feel more \"realistic\".",
    "authors": [
        "JayCeeCreates"
    ],
    "contact": {
        "homepage": "https://jayceecreates.github.io/earlygame/",
        "sources": "https://github.com/JayCeeCreates/earlygame/",
        "issues": "https://github.com/JayCeeCreates/earlygame/issues/"
    },

    "license": "MIT",
    "icon": "assets/earlygame/icon.png",
  
    "environment": "*",
    "entrypoints": {
        "main": [
            "jayceecreates.earlygame.EarlyGame"
        ],
        "client": [
            "jayceecreates.earlygame.EarlyGameClient"
        ],
        "modmenu": [
            "jayceecreates.earlygame.config.ModMenuIntegration"
        ]
    },
    "mixins": [
        "earlygame.mixins.json"
    ],
  
    "depends": {
        "fabricloader": ">=0.14.8",
        "fabric": "*",
        "minecraft": "1.19",
        "java": ">=16"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/earlygame",
                "modmenu.modrinth": "https://modrinth.com/mod/earlygame",
                "modmenu.wiki": "https://jayceecreates.github.io/earlygame/wiki/home.html"
            }
        }
    }
}
